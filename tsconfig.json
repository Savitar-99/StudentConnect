{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "strict": true,
    "jsx": "react-jsx", // Configuración necesaria para JSX con React
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node", // Resuelve correctamente los módulos
    "baseUrl": "./",
    "paths": {
      "*": ["src/*"]
    }
  }
}
